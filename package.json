{
  "name": "client_portal-aggregator_frontend",
  "version": "0.0.0",
  "description": "Frontend app for retailer products -> model name aggregation",
  "repository": "https://github.com/reevoo/client_portal-aggregator_frontend",
  "main": "index.js",
  "config": {
    "pact_port_cp_aggregator_api": 10014,
    "pact_port_cp_admin_api": 10015
  },
  "scripts": {
    "wait_for_port": "node scripts/wait_for_port.js",
    "pact:start:cp_aggregator": "bundle exec pact-mock-service start -p $npm_package_config_pact_port_cp_aggregator_api -l tests/pact/log/client_portal_aggregator_backend.log --pact-dir tests/pact/pacts --cors",
    "pact:start:cp_admin": "bundle exec pact-mock-service start -p $npm_package_config_pact_port_cp_admin_api -l tests/pact/log/api_client_portal-admin.log --pact-dir tests/pact/pacts --cors",
    "pact:start": "npm run pact:start:cp_aggregator ; npm run pact:start:cp_admin",
    "pact:stop:cp_aggregator": "bundle exec pact-mock-service stop -p $npm_package_config_pact_port_cp_aggregator_api",
    "pact:stop:cp_admin": "bundle exec pact-mock-service stop -p $npm_package_config_pact_port_cp_admin_api",
    "pact:stop": "npm run pact:stop:cp_aggregator ; npm run pact:stop:cp_admin",
    "pact:setup:cp_aggregator": "node tests/pact/setup/setup.js client_portal_aggregator_backend",
    "pact:setup:cp_admin": "node tests/pact/setup/setup.js api_client_portal-admin",
    "pact:setup": "npm run pact:setup:cp_aggregator ; npm run pact:setup:cp_admin",
    "pact:teardown:cp_aggregator": "node tests/pact/setup/teardown.js client_portal_aggregator_backend",
    "pact:teardown:cp_admin": "node tests/pact/setup/teardown.js api_client_portal-admin",
    "pact:teardown": "npm run pact:teardown:cp_aggregator ; npm run pact:teardown:cp_admin",
    "clean": "rm -rf dist",
    "build": "npm run clean && webpack",
    "ci": "TEST_ENV=ci npm run test && npm run build",
    "start": "webpack-dev-server",
    "test:contract": "npm run pact:start && npm run wait_for_port -- -p $npm_package_config_pact_port_cp_aggregator_api,$npm_package_config_pact_port_cp_admin_api && npm run pact:setup && karma start tests/karma.conf.contract.js && npm run pact:teardown && npm run pact:stop",
    "test:unit": "karma start tests/karma.conf.unit.js",
    "test:unit:watch": "karma start tests/karma.conf.unit.js --autoWatch=true --singleRun=false",
    "test": "npm run test:unit && npm run test:contract",
    "invalidate": "node scripts/cloudfront_invalidate.js",
    "deploy:staging": "NODE_ENV=staging npm run build && aws s3 sync dist s3://client-portal-staging/aggregator --acl=public-read && NODE_ENV=staging npm run invalidate",
    "deploy:production": "NODE_ENV=production npm run build && aws s3 sync dist s3://client-portal-production/aggregator --acl=public-read --cache-control=60 && NODE_ENV=production npm run invalidate"
  },
  "author": "Alexandru Anca and Maciej Palinski",
  "license": "ISC",
  "dependencies": {
    "client_portal-assets": "^0.6.0",
    "js-cookie": "^2.1.2",
    "material-ui": "^0.16",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0",
    "react-router": "^2.6.0",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.5.2",
    "redux-router": "^2.1.2",
    "redux-thunk": "^2.2",
    "whatwg-fetch": "^2.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.10.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-rewire": "^1.0.0-rc-4",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "css-loader": "^0.26.1",
    "dotenv": "^4.0.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-jsx": "^3.2.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-loader": "^1.4.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^5.9",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.4.1",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-ajax": "^0.1.13",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8",
    "node-sass": "^4.5",
    "pact-consumer-js-dsl": "^0.2.2",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.4",
    "redux-mock-store": "^1.2",
    "resolve-url-loader": "^1.6",
    "sass-loader": "^4.0.0",
    "sass-variable-loader": "0.0.4",
    "style-loader": "^0.13.1",
    "tcp-port-used": "^0.1.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.1"
  }
}
